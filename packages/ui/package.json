{
  "name": "@repo/ui",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist/**"
  ],
  "type": "module",
  "exports": {
    "./ads": {
      "types": "./dist/ads.d.ts",
      "import": "./dist/ads.js"
    },
    "./controls": {
      "types": "./dist/controls.d.ts",
      "import": "./dist/controls.js"
    },
    "./data": {
      "types": "./dist/data.d.ts",
      "import": "./dist/data.js"
    },
    "./header": {
      "types": "./dist/header.d.ts",
      "import": "./dist/header.js"
    },
    "./interactive-map": {
      "types": "./dist/interactive-map.d.ts",
      "import": "./dist/interactive-map.js"
    },
    "./providers": {
      "types": "./dist/providers.d.ts",
      "import": "./dist/providers.js"
    },
    "./overwolf": {
      "types": "./dist/overwolf.d.ts",
      "import": "./dist/overwolf.js"
    },
    "./styles/globals.css": "./dist/index.css"
  },
  "scripts": {
    "build": "tailwindcss -i ./src/styles/globals.css -o ./dist/index.css && tsup --format esm --dts --external react",
    "clean": "rm -rf dist",
    "dev": "run-p dev:tailwindcss dev:tsup",
    "dev:tailwindcss": "tailwindcss -i ./src/styles/globals.css -o ./dist/index.css --watch",
    "dev:tsup": "tsup --format esm --watch --dts --external react",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/",
    "test": "jest"
  },
  "jest": {
    "preset": "@repo/jest-presets/jest/browser"
  },
  "devDependencies": {
    "@overwolf/types": "3.24.7",
    "@repo/eslint-config": "workspace:*",
