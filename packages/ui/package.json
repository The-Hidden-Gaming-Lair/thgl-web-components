      "import": "./src/components/(overwolf)/index.tsx"
    },
    "./thgl-app": {
      "import": "./src/components/(thgl-app)/index.tsx"
    },
    "./styles/globals.css": "./dist/index.css",
    "./dicts/en.json": "./src/dicts/en.json"
  },
  "scripts": {
    "build": "tailwindcss -i ./src/styles/globals.css -o ./dist/index.css",
