    "@radix-ui/react-tooltip": "1.1.3",
    "@repo/lib": "workspace:*",
    "@tanstack/react-table": "^8.20.5",
    "@uidotdev/usehooks": "2.4.1",
    "class-variance-authority": "0.7.0",
