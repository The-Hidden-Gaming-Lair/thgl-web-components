  "type": "module",
  "exports": {
    "./ads": {
      "import": "./src/components/(ads)/index.tsx"
    },
    "./controls": {
      "import": "./src/components/(controls)/index.tsx"
    },
    "./content": {
      "import": "./src/components/(content)/index.tsx"
    },
