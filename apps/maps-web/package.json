{
  "name": "maps-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "reference": "rm -r public && mkdir public && cd public && ln -s ../../../static/global/icons global_icons && cd ..",
    "dereference": "cp -rL public public_temp && rm -r public && mv public_temp public",
    "build": "bun run reference && bun run dereference && next build",
    "clean": "rm -rf .next",
    "dev": "next dev -p 11001",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "start": "next start"
  },
  "dependencies": {
    "@auth/pg-adapter": "^1.4.1",
    "@repo/lib": "workspace:*",
    "@repo/ui": "workspace:*",
    "@vercel/postgres": "^0.9.0",
    "next": "14.2.4",
    "next-auth": "^5.0.0-beta.18",
    "pg": "^8.12.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "14.2.4",
    "@repo/eslint-config": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "20.14.10",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "postcss": "8.4.39",
    "tailwindcss": "3.4.4",
    "typescript": "5.5.3"
  }
}