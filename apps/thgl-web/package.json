{
  "name": "thgl-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "reference": "cd public && rm -r global_icons && ln -s ../../../static/global/icons global_icons && cd ..",
    "dereference": "cp -rL public public_temp && rm -r public && mv public_temp public",
    "build": "bun run reference && bun run dereference && next build",
    "clean": "rm -rf .next",
    "dev": "next dev -p 3006",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "start": "next start"
  },
  "dependencies": {
    "@repo/lib": "workspace:*",
    "@repo/ui": "workspace:*",
    "@vercel/blob": "^0.23.4",
    "@vercel/kv": "2.0.0",
    "jsonwebtoken": "9.0.2",
    "next": "14.2.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-markdown": "9.0.1",
    "remark-gfm": "4.0.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "14.2.4",
    "@repo/eslint-config": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/jsonwebtoken": "9.0.6",
    "@types/node": "20.14.10",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "postcss": "8.4.39",
    "tailwindcss": "3.4.4",
    "typescript": "5.5.3",
    "vercel": "^35.2.2"
  }
}