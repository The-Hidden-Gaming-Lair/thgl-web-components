{
  "name": "thgl-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "reference": "cd public && rm -r global_icons && ln -s ../../../static/global/icons global_icons && cd ..",
    "dereference": "cp -rL public public_temp && rm -r public && mv public_temp public",
    "build": "bun run reference && bun run dereference && next build",
    "clean": "rm -rf .next",
    "dev": "next dev -p 3006",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "start": "next start"
  },
  "dependencies": {
    "@repo/lib": "workspace:*",
    "@repo/ui": "workspace:*",
    "@vercel/kv": "1.0.1",
    "jsonwebtoken": "9.0.2",
    "next": "14.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-markdown": "9.0.1",
    "remark-gfm": "4.0.0"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "14.2.2",
    "@repo/eslint-config": "workspace:*",
    "@repo/tailwind-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/jsonwebtoken": "9.0.6",
    "@types/node": "20.12.7",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "autoprefixer": "10.4.19",
    "postcss": "8.4.38",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5"
  }
}